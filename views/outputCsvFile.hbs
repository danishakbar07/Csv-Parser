<style>
    table {
    width: 100%;
    border-collapse: collapse;
    font-family: Arial, sans-serif;
    font-size: 14px;
}

thead {
    background-color: #f2f2f2;
}

th, td {
    border: 1px solid #ddd;
    padding: 8px;
    text-align: left;
}

th {
    font-weight: bold;
}

tr:nth-child(even) {
    background-color: #f9f9f9;
}

tr:hover {
    background-color: #f1f1f1;
}

th, td {
    padding: 12px 15px;
}
.break-word{
    word-break: break-all;
}

</style>
<button st><a href="/">Back to home</a></button>
<table style="margin-top: 4rem;">
    <thead>
        <tr>
         <th>SL.NO</th>
        <th>Product Name</th>
        <th>Input Urls</th>
        <th>Status</th>
        <th>Output Urls</th>
        </tr>
      
    </thead>
    <tbody>
        
        {{#each this}}
        <tr>
  <td>{{slNo}}</td>
        <td>{{productName}}</td>
        <td class="break-word">{{inputImgUrls}}</td>
        <td>{{status}}</td>
        <td class="break-word">{{outputImgUrls}}</td>
        </tr>
      
        {{/each}}
    </tbody>
</table>
